RewriteEngine on 
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [NC,L]
RewriteRule ^storitve$ /#storitve [R=301,L]
RewriteRule ^pogosta-vprasanja$ /#vprasanja [R=301,L]
RewriteRule ^kontakt$ /#kontakt [R=301,L]
RewriteRule ^izracun-cene$ /calc [R=301,L]